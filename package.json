{
  "name": "apollo-server-quick",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "ts-node index.ts",
    "dev": "nodemon index.ts",
    "debug": "nodemon --exec 'node --inspect=0.0.0.0:9930 --require ts-node/register index.ts'",
    "sync": "ts-node scripts/syncSchema",
    "config": "ts-node scripts/pullConfig",
    "migrate": "sequelize db:migrate --config ./config/db.json --migrations-path ./db/migrations",
    "migrate:new": "sequelize migration:generate --config ./config/db.json --migrations-path ./db/migrations --name $name",
    "migrate:undo": "sequelize db:migrate:undo --config ./config/db.json --migrations-path ./db/migrations"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",

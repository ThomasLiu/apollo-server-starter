    "graphql-tag": "^2.10.1",
    "graphql-type-json": "^0.3.0",
    "lodash": "^4.17.11",
    "nodemon": "^1.19.1",
    "pg": "^6.4.2",
    "pg-hstore": "^2.3.2",
    "rimraf": "^2.6.3",
    "sequelize": "^5.8.7",
    "sequelize-auto": "^0.4.29",
    "sequelize-cli": "^5.4.0",
    "ts-node": "^8.2.0",
    "validator": "^11.0.0"
  },
  "peerDependencies": {
    "graphql": "^0.13.0 || ^14.0.0"
  },

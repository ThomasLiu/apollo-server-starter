    "@types/bluebird": "^3.5.27",
    "@types/consul": "^0.23.33",
    "@types/graphql": "^14.2.0",
    "@types/graphql-iso-date": "^3.3.1",
    "@types/graphql-type-json": "^0.3.0",
    "@types/koa": "^2.0.48",
    "@types/lodash": "^4.14.132",
    "@types/node": "^12.0.2",
    "@types/rimraf": "^2.0.2",
    "@types/sequelize": "^4.28.2",
    "apollo-server-koa": "^2.5.0",
    "axios": "^0.19.0",
    "bluebird": "^3.5.5",
    "consul": "^0.34.1",
    "dataloader-sequelize": "^1.7.10",
